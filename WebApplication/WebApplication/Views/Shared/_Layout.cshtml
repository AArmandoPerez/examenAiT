<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Examen Armando</title>



    <link href="~/Content/DataTables-1.10.18/css/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/Content/DataTables-1.10.18/css/dataTables.checkboxes.css" rel="stylesheet" />
    <link href="~/Content/DataTables-1.10.18/css/select.dataTables.min.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/moment.js"></script>


    <script src="~/Content/DataTables-1.10.18/js/jquery.dataTables.js"></script>
    <script src="~/Content/DataTables-1.10.18/js/dataTables.buttons.min.js"></script>
    <script src="~/Content/DataTables-1.10.18/js/buttons.flash.min.js"></script>
    <script src="~/Content/DataTables-1.10.18/js/jszip.min.js"></script>
    <script src="~/Content/DataTables-1.10.18/js/pdfmake.min.js"></script>
    <script src="~/Content/DataTables-1.10.18/js/vfs_fonts.js"></script>
    <script src="~/Content/DataTables-1.10.18/js/buttons.html5.min.js"></script>
    <script src="~/Content/DataTables-1.10.18/js/buttons.print.min.js"></script>
    <script src="~/Content/DataTables-1.10.18/js/dataTables.checkboxes.min.js"></script>
    <script src="~/Content/DataTables-1.10.18/js/dataTables.select.min.js"></script>



    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Examen Armando", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Inicio", "Index", "Home")</li>
                    @*<li>@Html.ActionLink("Acerca de", "About", "Home")</li>
                        <li>@Html.ActionLink("Contacto", "Contact", "Home")</li>*@
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        <br />
        <br />
        @RenderBody()
        <hr />
        <br />
        <footer>
            <p>&copy; @DateTime.Now.Year -Examen Armando</p>
        </footer>
    </div>
</body>
</html>
